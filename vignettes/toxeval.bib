	month = apr,
	year = {2012},
	keywords = {Zebrafish, Chemical class, Concentration-response, Development, Octanol-water partition coefficient, ToxCast™},
	pages = {174--187},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\srcorsi\\Zotero\\storage\\V6CR9IUU\\Padilla et al. - 2012 - Zebrafish developmental screening of the ToxCast™ .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\srcorsi\\Zotero\\storage\\5RV7ZC9M\\S0890623811004011.html:text/html}
