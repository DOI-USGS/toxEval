	file = {Full Text PDF:C\:\\Users\\srcorsi\\Zotero\\storage\\BZJAYCHE\\Shannon et al. - 2003 - Cytoscape A Software Environment for Integrated M.pdf:application/pdf;Snapshot:C\:\\Users\\srcorsi\\Zotero\\storage\\MTFV32FP\\2498.html:text/html}
